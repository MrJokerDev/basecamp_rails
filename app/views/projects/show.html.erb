<%= render 'navbar/navbar' %>
<h1>Project show</h1>

<%= link_to 'Back', projects_path, :class => "card-link btn btn-secondary mt-3 mb-3" %>

<div class="card">
    <div class="card-header">
        <%= @project.title %>
    </div>
    <div class="card-body">
        <blockquote class="blockquote mb-0">
            <footer class="blockquote-footer"><%= @project.body %></footer>
        </blockquote>
    </div>
</div>